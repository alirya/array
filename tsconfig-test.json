{
  "compilerOptions": {
    "removeComments" : false,
    "moduleResolution": "node",
    "module": "commonjs",
    "declaration": true,
    "target": "es2016",
    "outDir": "dist-test",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "sourceMap": true,
    "downlevelIteration" : true,
    "strictNullChecks": true,
    "rootDir" : "test",
    "incremental": true,
    "tsBuildInfoFile": "./dist-test/compiler-cache",
    "strictPropertyInitialization": true,
    "lib": [
      "es2016",
      "es2015",
      "dom",
      "es5"
    ]
  },
  "include":  [
    "test/**/*"
  ],
  "exclude":   [
  ]

}
